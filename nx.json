{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "installation": {
    "version": "latest",
    "plugins": {
      "sources": []
    }
  },
  "defaultBase": "main",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ],
    "production": [
      "!{projectRoot}/**/*.spec.ts",
      "!{projectRoot}/**/*.test.ts"
    ]
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "format": {
      "cache": false
    },
    "dev": {
      "cache": false
    }
  },
  "parallel": 3
}