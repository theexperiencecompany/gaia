{
    "name": "mobile",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/mobile",
    "projectType": "application",
    "targets": {
        "dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm start",
                "cwd": "apps/mobile"
            }
        },
        "android": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm android",
                "cwd": "apps/mobile"
            }
        },
        "ios": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm ios",
                "cwd": "apps/mobile"
            }
        },
        "web": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm web",
                "cwd": "apps/mobile"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "pnpm lint",
                "cwd": "apps/mobile"
            }
        },
        "clean": {
            "executor": "nx:run-commands",
            "options": {
                "command": "rm -rf .expo node_modules",
                "cwd": "apps/mobile"
            }
        }
    },
    "tags": [
        "type:app",
        "platform:mobile"
    ]
}