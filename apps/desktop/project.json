{
  "name": "desktop",
  "root": "apps/desktop",
  "sourceRoot": "apps/desktop/src",
  "projectType": "application",
  "targets": {
    "sync": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm install --frozen-lockfile --prefer-offline",
        "cwd": "{workspaceRoot}"
      },
      "cache": true,
      "inputs": [
        "{workspaceRoot}/pnpm-lock.yaml",
        "{workspaceRoot}/package.json",
        "{projectRoot}/package.json"
      ],
      "outputs": []
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx dev web", "pnpm dev"],
        "cwd": "{projectRoot}",
        "parallel": true
      },
      "dependsOn": []
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "{projectRoot}"
      },
      "cache": true,
      "dependsOn": [],
      "inputs": [
        "default",
        "^production",
        "{projectRoot}/electron.vite.config.ts",
        "{projectRoot}/electron-builder.yml"
      ],
      "outputs": ["{projectRoot}/out", "{projectRoot}/dist"]
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm start",
        "cwd": "{projectRoot}"
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx build web --skip-nx-cache && pnpm pack",
        "cwd": "{projectRoot}",
        "env": {
          "NEXT_PUBLIC_API_BASE_URL": "https://api.heygaia.io/api/v1/"
        }
      },
      "dependsOn": ["build"]
    },
    "dist": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx build web --skip-nx-cache && pnpm dist",
        "cwd": "{projectRoot}",
        "env": {
          "NEXT_PUBLIC_API_BASE_URL": "https://api.heygaia.io/api/v1/"
        }
      },
      "dependsOn": ["build"]
    },
    "dist:mac": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx build web --skip-nx-cache",
          "./scripts/prepare-next-server.sh",
          "pnpm dist:mac"
        ],
        "cwd": "{projectRoot}",
        "parallel": false,
        "env": {
          "NEXT_PUBLIC_API_BASE_URL": "https://api.heygaia.io/api/v1/"
        }
      },
      "dependsOn": ["build"]
    },
    "dist:win": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx build web --skip-nx-cache && pnpm dist:win",
        "cwd": "{projectRoot}",
        "env": {
          "NEXT_PUBLIC_API_BASE_URL": "https://api.heygaia.io/api/v1/"
        }
      },
      "dependsOn": ["build"]
    },
    "dist:linux": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx build web --skip-nx-cache && pnpm dist:linux",
        "cwd": "{projectRoot}",
        "env": {
          "NEXT_PUBLIC_API_BASE_URL": "https://api.heygaia.io/api/v1/"
        }
      },
      "dependsOn": ["build"]
    },
    "type-check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm type-check",
        "cwd": "{projectRoot}"
      },
      "cache": true,
      "inputs": ["default", "{projectRoot}/tsconfig.json", "^production"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm lint",
        "cwd": "{projectRoot}"
      },
      "cache": true,
      "inputs": ["default", "{workspaceRoot}/biome.json"]
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm lint:fix",
        "cwd": "{projectRoot}"
      },
      "cache": false
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm clean",
        "cwd": "{projectRoot}"
      }
    }
  },
  "implicitDependencies": ["web"]
}
